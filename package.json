{
  "name": "@heroku-cli/plugin-api",
  "description": "access the Heroku API directly",
  "version": "2.3.0",
  "author": "Heroku",
  "bugs": {
    "url": "https://github.com/heroku/heroku-api-plugin/issues"
  },
  "engines": {
    "node": ">= 16.0"
  },
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^11.4.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "cli-ux": "^4.6.1",
    "edit-string": "1.1.6",
    "fs-extra": "^9.0.1",
    "get-stdin": "8.0.0",
    "http-call": "^5.3.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.6",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "16.11.7",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.4.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-oclif": "^0.1.0",
    "globby": "^13.2.2",
    "mocha": "^10.4.0",
    "nock": "^13.5.1",
    "oclif": "3.17.2",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  },
  "files": [
    "oclif.manifest.json",
    "lib"
  ],
  "homepage": "https://github.com/heroku/heroku-api-plugin",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "ISC",
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "watch-extensions": "ts",
    "recursive": true,
    "reporter": "spec",
    "timeout": 360000
  },
  "oclif": {
    "bin": "heroku",
    "commands": "./lib/commands"
  },
  "repository": "heroku/heroku-api-plugin",
  "resolutions": {
    "isbinaryfile": "4.0.10"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "postpublish": "rm oclif.manifest.json",
    "posttest": "yarn lint && tsc --noEmit -p test",
    "prepare": "rm -rf lib && tsc",
    "prepack": "tsc && oclif manifest",
    "pretest": "tsc && oclif manifest",
    "test": "mocha test/**/*.test.ts",
    "version": "oclif readme && git add README.md"
  }
}
